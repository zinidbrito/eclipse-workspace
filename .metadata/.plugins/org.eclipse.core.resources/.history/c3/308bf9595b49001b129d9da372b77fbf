import java.util.Locale;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

		Scanner sc = new Scanner(System.in);		
		
		System.out.println("Digite o nome do outorgante...");
		String outorgante = sc.toString();
		
		System.out.println("Digite o Número do Livro...");
		int livro = sc.nextInt();
		
		System.out.println("Digite o numero da folha...");
		int folha = sc.nextInt();
		
		sc.close();
		}

		

		

	}
