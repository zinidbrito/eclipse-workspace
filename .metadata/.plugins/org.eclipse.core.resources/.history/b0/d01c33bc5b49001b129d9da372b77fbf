import java.util.Locale;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

		Scanner sc = new Scanner(System.in);		
		
		System.out.println("Digite o nome do outorgante...");
		String outorgante = sc.nextLine();
		
		System.out.println("Digite o Número do Livro...");
		int livro = sc.nextInt();
		
		System.out.println("Digite o numero da folha...");
		int folha = sc.nextInt();
		
		
		System.out.println("Outorgante " + outorgante + " Livro : " + livro + " Número da Folha " + folha + " BUSCA EM ATÉ 5 DIAS ÚTEIS");
		sc.close();
		}

		

		

	}
